extends layout

block content
  h1= title
  p Welcome to #{title}
  p sai sravani veeranki
  
  button(type="button", class="btn btn-secondary", onclick='goFaster()') press me

  div(id="faster") Increase the speed 
  script.
    var mph = 20; 
    var change = 1;
    function goFaster(){
      mph += change ;
      change ++ ;
      var div = document.getElementById('faster');
      div.textContent = `MPH ${mph} with change ${change}` ;
    }

  a(href='/Animals') View Animals Results